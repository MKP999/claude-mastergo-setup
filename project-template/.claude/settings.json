{
  "mcpServers": {
    "mastergo-magic-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@mastergo/magic-mcp",
        "--token=YOUR_TOKEN_HERE",
        "--url=https://mastergo.com"
      ]
    }
  },
  "permissions": {
    "allow": [
      "mcp__mastergo-magic-mcp__mcp__getDsl",
      "mcp__mastergo-magic-mcp__mcp__getComponentLink",
      "mcp__mastergo-magic-mcp__mcp__getMeta",
      "mcp__mastergo-magic-mcp__mcp__getComponentGenerator"
    ]
  }
}
